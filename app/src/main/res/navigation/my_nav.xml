<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.musicmeditateplayer.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_breathFragment2"
            app:destination="@id/breathFragment" />
        <action
            android:id="@+id/action_homeFragment_to_napFragment2"
            app:destination="@id/napFragment" />
    </fragment>
    <fragment
        android:id="@+id/sleepFragment"
        android:name="com.example.musicmeditateplayer.SleepFragment"
        android:label="fragment_sleep"
        tools:layout="@layout/fragment_sleep" />
    <fragment
        android:id="@+id/meditateFragment"
        android:name="com.example.musicmeditateplayer.MeditateFragment"
        android:label="fragment_meditate"
        tools:layout="@layout/fragment_meditate" />
    <fragment
        android:id="@+id/moreFragment"
        android:name="com.example.musicmeditateplayer.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" />
    <fragment
        android:id="@+id/breathFragment"
        android:name="com.example.musicmeditateplayer.BreathFragment"
        android:label="fragment_breath"
        tools:layout="@layout/fragment_breath" />
    <fragment
        android:id="@+id/napFragment"
        android:name="com.example.musicmeditateplayer.NapFragment"
        android:label="fragment_nap"
        tools:layout="@layout/fragment_nap" />
</navigation>